

<body>


{{> main_with_sidebar_start}}

<div class="col-sm-3 col-lg-2">
    <nav>

        {{#each db}}
            <p>{{this.name}}</p>
            <ul class="dropdown">
                {{#each this.sections}}
                    <li><a href="#">{{this}}</a></li>
                {{/each}}
            </ul>
        {{/each}}
    </nav>
</div>




{{> main_with_sidebar_end}}



</body>